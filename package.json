{
  "name": "app-template",
  "version": "15.6.0",
  "description": "Angular app template",
  "main": "app/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/paradox41/app-template"
  },
  "author": "Will <will.soto9+github@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/paradox41/app-template",
  "bugs": {
    "url": "https://github.com/paradox41/app-template/issues"
  },
  "keywords": [
    "angular",
    "webpack",
    "mocha",
    "chai",
    "karma",
    "conventional changelog",
    "boilerplate"
  ],
  "scripts": {
    "clean": "rimraf build/",
    "prestart": "npm run clean",
    "start": "webpack-dev-server --hot --inline --open",
    "test": "karma start ./config/karma.conf.js",
    "test:watch": "npm test -- --auto-watch --no-single-run",
    "prebuild": "npm run clean",
    "build": "webpack --config ./config/webpack.prod.babel.js --progress",
    "prerelease": "npm run ci",
    "release": "standard-version --sign && git push --follow-tags origin master",
    "coveralls": "coveralls < coverage/lcov.info",
    "postcoveralls": "rimraf ./coverage",
    "postinstall": "webdriver-manager update",
    "pree2e": "npm run build",
    "e2e": "protractor e2e/protractor.conf.js",
    "ci": "npm test && npm run e2e",
    "precommit": "lint-staged",
    "commitmsg": "validate-commit-msg"
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --print-width 120 --write",
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@uirouter/angularjs": "^1.0.3",
    "angular": "^1.6.4",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "angular-mocks": "^1.6.4",
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-angularjs-annotate": "^0.7.0",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-lodash": "^3.2.11",
    "babel-preset-env": "^1.5.1",
    "chai": "^4.0.1",
    "chai-as-promised": "^7.0.0",
    "chalk": "^1.1.3",
    "coveralls": "^2.13.1",
    "css-loader": "^0.28.4",
    "cssnano": "^3.10.0",
    "eslint": "^3.19.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.3.0",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.13.4",
    "karma": "^1.7.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-coverage": "^1.1.1",
    "karma-coveralls": "^1.1.2",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.31",
    "karma-webpack": "^2.0.3",
    "lint-staged": "^3.6.0",
    "lodash-webpack-plugin": "^0.11.4",
    "mocha": "^3.4.2",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.5",
    "postcss-reporter": "^4.0.0",
    "prettier": "^1.3.1",
    "protractor": "^5.1.2",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.5",
    "standard-version": "^4.0.0",
    "style-loader": "^0.18.1",
    "stylelint": "^7.10.1",
    "stylelint-config-standard": "^16.0.0",
    "validate-commit": "^3.2.1",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-merge": "^4.1.0"
  }
}
