{
  "name": "app-template",
  "version": "15.1.0",
  "description": "Angular app template",
  "main": "app/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/paradox41/app-template"
  },
  "author": "Will <will.soto9+github@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/paradox41/app-template",
  "bugs": {
    "url": "https://github.com/paradox41/app-template/issues"
  },
  "keywords": [
    "angular",
    "gulp",
    "template",
    "webpack",
    "starter",
    "mocha",
    "chai",
    "karma",
    "conventional changelog"
  ],
  "scripts": {
    "clean": "rimraf build/",
    "prestart": "npm run clean",
    "start": "webpack-dev-server --hot --inline --https --open",
    "test": "NODE_ENV=test karma start ./config/karma.conf.js",
    "test:watch": "npm test -- --auto-watch --no-single-run",
    "prebuild": "npm run clean",
    "build": "webpack --config ./config/webpack.prod.js --progress",
    "prerelease": "npm test && npm run protractor",
    "release": "standard-version --sign && git push --follow-tags origin master",
    "coveralls": "coveralls < coverage/lcov.info",
    "postcoveralls": "rimraf ./coverage",
    "postinstall": "webdriver-manager update",
    "preprotractor": "npm run build",
    "protractor": "protractor e2e/protractor.conf.js",
    "ci": "npm test && npm run protractor"
  },
  "git": {
    "scripts": {
      "pre-commit": "git diff --cached --name-only --diff-filter=ACM | grep '.js$' | xargs ./node_modules/.bin/eslint",
      "commit-msg": "./node_modules/.bin/validate-commit-msg $1"
    }
  },
  "dependencies": {
    "angular": "^1.5.5",
    "angular-ui-router": "^1.0.0-beta.1",
    "lodash": "^4.12.0"
  },
  "devDependencies": {
    "angular-mocks": "^1.5.5",
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-__coverage__": "^11.0.0",
    "babel-plugin-lodash": "^3.2.3",
    "babel-preset-es2015": "^6.9.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "conventional-changelog": "^1.1.0",
    "conventional-changelog-cli": "^1.2.0",
    "copy-webpack-plugin": "^3.0.0",
    "coveralls": "^2.11.9",
    "css-loader": "^0.23.1",
    "del": "^2.2.0",
    "eslint": "^3.2.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^1.12.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.3",
    "git-scripts": "^0.2.1",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.17.0",
    "json-loader": "^0.5.4",
    "karma": "^1.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.1.0",
    "karma-coveralls": "^1.1.2",
    "karma-mocha": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.7.0",
    "lodash-webpack-plugin": "^0.10.0",
    "minimist": "^1.2.0",
    "mocha": "^3.0.0",
    "ng-annotate-loader": "^0.1.0",
    "node-sass": "^3.7.0",
    "postcss-loader": "^0.10.0",
    "protractor": "^4.0.3",
    "rimraf": "^2.5.2",
    "run-sequence": "^1.2.0",
    "sass-loader": "^4.0.0",
    "standard-version": "^2.3.0",
    "style-loader": "^0.13.1",
    "stylelint": "^7.1.0",
    "stylelint-config-standard": "^12.0.0",
    "stylelint-webpack-plugin": "^0.3.0",
    "validate-commit": "^2.2.0",
    "webpack": "^2.1.0-beta.20",
    "webpack-dev-server": "^2.1.0-beta.0",
    "webpack-merge": "^0.14.0"
  }
}
