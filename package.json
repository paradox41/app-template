{
  "name": "app-template",
  "version": "15.4.0",
  "description": "Angular app template",
  "main": "app/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/paradox41/app-template"
  },
  "author": "Will <will.soto9+github@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/paradox41/app-template",
  "bugs": {
    "url": "https://github.com/paradox41/app-template/issues"
  },
  "keywords": [
    "angular",
    "webpack",
    "mocha",
    "chai",
    "karma",
    "conventional changelog",
    "boilerplate"
  ],
  "scripts": {
    "clean": "rimraf build/",
    "prestart": "npm run clean",
    "start": "webpack-dashboard -c cyan -- webpack-dev-server --hot --inline --https --open",
    "test": "karma start ./config/karma.conf.js",
    "test:watch": "npm test -- --auto-watch --no-single-run",
    "prebuild": "npm run clean",
    "build": "webpack --config ./config/webpack.prod.babel.js --progress",
    "prerelease": "npm test && npm run protractor",
    "release": "standard-version --sign && git push --follow-tags origin master",
    "coveralls": "coveralls < coverage/lcov.info",
    "postcoveralls": "rimraf ./coverage",
    "postinstall": "webdriver-manager update",
    "preprotractor": "npm run build",
    "protractor": "protractor e2e/protractor.conf.js",
    "ci": "npm test && npm run protractor"
  },
  "git": {
    "scripts": {
      "pre-commit": "git diff --cached --name-only --diff-filter=ACM | grep '.js$' | xargs ./node_modules/.bin/eslint",
      "commit-msg": "./node_modules/.bin/validate-commit-msg $1"
    }
  },
  "dependencies": {
    "angular": "^1.6.1",
    "angular-ui-router": "^1.0.0-beta.2",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "angular-mocks": "^1.6.1",
    "autoprefixer": "^6.6.1",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-angularjs-annotate": "^0.6.0",
    "babel-plugin-istanbul": "^3.1.2",
    "babel-plugin-lodash": "^3.2.11",
    "babel-preset-latest": "^6.16.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chalk": "^1.1.3",
    "copy-webpack-plugin": "^4.0.1",
    "coveralls": "^2.11.15",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.2",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.3",
    "git-scripts": "^0.2.1",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.26.0",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-coveralls": "^1.1.2",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.8.1",
    "lodash-webpack-plugin": "^0.10.7",
    "mocha": "^3.2.0",
    "node-sass": "^4.1.1",
    "postcss-loader": "^1.2.1",
    "protractor": "^4.0.14",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.1.1",
    "standard-version": "^4.0.0",
    "style-loader": "^0.13.1",
    "stylelint": "^7.7.1",
    "stylelint-config-standard": "^15.0.1",
    "stylelint-webpack-plugin": "^0.4.2",
    "validate-commit": "^2.3.2",
    "webpack": "beta",
    "webpack-dashboard": "^0.2.1",
    "webpack-dev-server": "beta",
    "webpack-merge": "^2.2.0"
  }
}
